{
  "name": "plr",
  "version": "1.0.13",
  "description": "build tool angular frontend projects",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "plr": "dist/cli.js"
  },
  "scripts": {
    "test": "(npm run prepareTest && npmts --notest && npm run plrTest && npm run plrTestInstall && npm run plrTestServe)",
    "plrTest": "(cd ./test/ && node ../dist/cli.js)",
    "plrTestInstall": "(cd ./test/ && node ../dist/cli.js install)",
    "plrTestServe": "(cd ./test/ && node ../dist/cli.js serve)",
    "prepareTest": "(rm -rf test/ && mkdir test && cd test/ && git clone https://gitlab.com/sandboxzone/sandbox-plr.git .)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushrocks/plr.git"
  },
  "keywords": [
    "polymer",
    "typescript"
  ],
  "author": "Lossless GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pushrocks/plr/issues"
  },
  "homepage": "https://github.com/pushrocks/plr#readme",
  "devDependencies": {
    "@types/should": "^8.1.30",
    "should": "^11.1.1",
    "typings-test": "^1.0.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.40",
    "@types/q": "0.0.32",
    "@types/shelljs": "^0.3.33",
    "beautylog": "^6.0.0",
    "early": "^2.0.35",
    "lodash": "^4.17.2",
    "npmextra": "^2.0.3",
    "npmts-g": "^5.2.8",
    "q": "^1.4.1",
    "shelljs": "^0.7.5",
    "smartbrowser": "^1.0.1",
    "smartcli": "1.0.15",
    "smartfile": "^4.1.0",
    "smartjspm": "^1.0.7",
    "smartpath": "^3.2.5",
    "smartsystem": "^1.0.9",
    "taskbuffer": "^1.0.8",
    "tsn": "^2.0.13",
    "typings-global": "^1.0.14"
  }
}
